@using AMMasterProject.ViewModel
@model List<ItemHomePageDesignSelectionMetaData>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}


@if(Model!=null)
{


     <div class="table-responsive text-nowrap border" style="height:300px; min-height:300px; overflow:auto">
            <table id="ListingLoad1" class="table">
                <thead class="table-secondary">
                    <tr>


                        <th style="width:50px" class="text-center">ID</th>
                        <th style="width:350px" class="left">Title</th>
                        <th>Mark Active</th>
                
                    </tr>
                </thead>
                <tbody class="table-border-bottom-0">
               
    
    @foreach (var item in Model)
    {     <tr>

                    <td>
                            @item.SelectionID
                        </td>
                        <td >
                        @item.Name</td>
                        <td class="link-active pull-right ">
                            <a class="active">
                                Active
                            </a>
                        </td>
         </tr>
    }

    </tbody>
    </table>
    </div>
}