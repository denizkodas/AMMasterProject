                            <td> @Model.DigitalClassified</td>
                            <td>@Model.AuctionDigital</td>
                            <td>@Model.PennyAuctionDigital</td>
                            <td class="black-c bold">@Model.TotalDigital</td>

                        </tr>

                        <tr>
                            <td class="black-c bold">Service</td>
