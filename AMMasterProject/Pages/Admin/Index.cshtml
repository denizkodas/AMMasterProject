                            <a href="/admin/usermanagement/seller?Itemlisting=1" target="_blank" s class="grey-c col-md-6 no-padding "> <span class="badge grey-bg">@Model.TotalSellersHaveProducts</span></a>
                        </div>
                        </div>
                </div>
            </div>
        </div>
       
        <div class="col-lg-8 col-md-12 col-6 mb-4">
