                            <a href="/admin/usermanagement/buyer?PurchaseItem=1" target="_blank" s class="grey-c col-md-6 no-padding "> <span class="badge grey-bg">@Model.TotalCustomersPurchaseProducts</span></a>
                        </div>   
                            <div class="col-md-6">
                        
