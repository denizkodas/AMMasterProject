        <script>
            const jsonDataPurchased = @Html.Raw(JsonConvert.SerializeObject(@creditModel.MonthsPurchasedJson));
            const jsonDataUsed = @Html.Raw(JsonConvert.SerializeObject(@creditModel.MonthsUsedJson));

            initializeChartCreditPurchased(jsonDataPurchased, jsonDataUsed);



            var transformedData = @Html.Raw(JsonConvert.SerializeObject(Model.BuyerView.SubscriptionPurchaseModel));
            const chartData = transformedData.map(item => ({
                x: item.PackageName,
                y: [
                    new Date(item.SubscriptionDate).getTime(),
                    new Date(item.RenewDate).getTime()
                ]
            }));

            // console.log(chartData);
            // Initialize the chart with transformed data
            initializeChartSubscription(chartData);




            ///seller order status
            initializeChartOrderSummary(@Model.S_ShippedCount, @Model.S_ProcessingCount, @Model.S_DeliveredCount, @Model.S_CompletedCount, @Model.S_CancelledCount, @Model.S_ReturnedCount);




            ///buyer order status
            initializeChartOrderSummaryBuyer(@Model.B_ShippedCount, @Model.B_ProcessingCount, @Model.B_DeliveredCount, @Model.B_CompletedCount, @Model.B_CancelledCount, @Model.B_ReturnedCount);

        </script>
