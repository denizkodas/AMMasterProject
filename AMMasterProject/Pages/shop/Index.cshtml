                                        <span class="notranslate" data-translate="membersince">Member Since</span>:
                                        <span data-translate="date">@Model.sellerview.InsertDate</span>
                                    </span>
                                    |
                                    @if (Model.sellerview.userothermetadata != null)
                                    {
                                        var offlineType = Model.sellerview.userothermetadata.UserInactiveMetaData?.OffLineType;
                                        var onlineStatusClass = offlineType == "online" ? "green-c" : "d-grey-c";
                                        var onlineStatusText = offlineType == "online" ? "Online" : "Offline";

                                        <span class="@onlineStatusClass s-bold"> • @onlineStatusText </span>
                                    }
                                    |  <span class="grey-c">
                                        @if (@Model.sellerview.primaryaddressViewModel != null)
                                        {
                                            @GlobalHelper.GetLocalTimeByCoordinatesAsync(@Model.sellerview.primaryaddressViewModel.TimeZone) <span> local time </span>
                                        }
                                    </span>


                                </div>
