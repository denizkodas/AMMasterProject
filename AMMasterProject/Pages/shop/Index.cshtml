@page "/user/{businessurlpath}"
@using AMMasterProject.Helpers;

@model AMMasterProject.Pages.shop.IndexModel
@{


    var productmodel = Model.productmodel.ProductList;

    // Define the title and meta tags


    var description = Model.sellerview != null ? Model.sellerview.BusinessDescription ?? Model.sellerview.BusinessName : null;

    // Set the page title
    ViewData["Title"] = Model.sellerview != null ? Model.sellerview.BusinessName : null;

    // Set the meta tags
    ViewData["Description"] = description;
    ViewData["Keywords"] = GlobalHelper.ExtractKeywords(description);
    ViewData["ImageUrl"] = Model.sellerview != null ? Model.sellerview.Image : "";

}
<style>
    body {
        background: white;
    }

    .main-header.main-header-classadd {
        position: relative
    }

</style>

<div class="seller-detail">

    @if (Model != null)
    {



        <div class="section-basic-info">
            <div class="s-cover-img">
                <img class="" src="@Model.sellerview.SellerCoverImage" />
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-md-8 no-padding">

                        <div class="basic-info">
