<script>

    $(document).ready(function () {
        var url = '@Model.sellerview.SellerVideoURl';
        setTimeout(function () {
            previewvideo(url);
        }, 1000); // 2000 milliseconds = 2 seconds
    });

    $("#afollowseller").click(function () {
        event.preventDefault();

        var loginid = $(this).data('loginid')

        var sellerid = $(this).data('sellerid')

        if (loginid != "0") {


            sendfollow(loginid, sellerid);

            // Get the current text of the anchor tag
            var currentText = $(this).text().trim();

            // Toggle the text between "Follow" and "UnFollow"
            var newText = (currentText == "Follow") ? "Un Follow" : "Follow";
            $("#lblfollowstatus").text(newText);

            var totalfollowers = parseInt($("#lblsellerfollowers").text()); // Get current total followers

            // Update the total followers count based on follow/unfollow action
            if (newText === "Un Follow") {
                totalfollowers += 1; // Decrease the counter by 1
            } else {
                totalfollowers -= 1; // Increase the counter by 1
            }

            $("#lblsellerfollowers").text(totalfollowers); // Update the total followers display

        } else {
