                                        <span class="s-count"> @productmodel.Count() </span>
                                    </span>

                                    <span class="s-followers">
                                        <span class="notranslate" data-translate="followers">Followers</span>




                                        <label id="lblsellerfollowers" class="s-count  kformat"> @Model.sellerview.userothermetadata.Followers </label>

                                    </span>
                                </div>

